    <script>

    //Run when page starts
    $(document).ready(function(){
      listRef()
    });
    //list refresh function
    function listRef() {
        $.ajax({
          url: '/client/makeList',
          method: 'post',
          success: function(result) {
            if(result){
              $('#select_data').empty();
	            for(var i=0 ; i<result.datalist.length ; i++){
                $('#select_data').append('<option value='+result.datalist[i].dataName+'>'+result.datalist[i].dataName+'</option>');
	            }
	          }
	        }
        })
      }
    //upload button function
    $('.data_upload').click(function(){
	    var form = $('#fileForm')[0]
      var formData = new FormData(form)
      var fileSize = fileInput.files[0].size

      //add file data to form
      formData.append("description",$('#description').val())
      formData.append("fileSize",fileSize)

	    if($('#fileInput').val() == ""){
          alert('please select upload file')
      }else{
    	  $.ajax({
  	      url:'/client/dataUpload',
	        processData: false,
          contentType: false,
          data : formData,
	        type: 'POST',
	        success: function(result){
            if(result){
              listRef()
            }
	        }
        })
	    }
    })
    //delete button function
    $('.delete').click( function() {
      if($('#select_data option:selected').val() == null){
          alert('please select data file')
      }else{
      $.ajax({
        url: '/client/dataDelete',
        method: 'post',
        data: {
	        'data' : $('#select_data option:selected').val()
      	},
        dataType: 'json',
        success: function(result) {
          if(result){
      	    listRef()
          }
        }
      })
      }
		})
    </script>
